@model RegisterResultModel
@{
    Layout = "_ColumnsOne";

    //title
    NopHtml.AddTitleParts(T("PageTitle.Register").Text);
    //page class
    NopHtml.AppendPageCssClassParts("html-registration-result-page");
}
<div class="page registration-result-page">
    <div class="page-title">
        <h1 style="text-align:center;padding-top:20px">@T("Account.Register")</h1>
    </div>
    <div class="page-body">
        <div class="result" style="text-align:center;padding-bottom:10px">
            @Model.Result
        </div>
        <div class="buttons">
            <a href="@Model.ReturnUrl" class="button-1 register-continue-button">@T("Common.Continue")</a>
        </div>
    </div>
</div>